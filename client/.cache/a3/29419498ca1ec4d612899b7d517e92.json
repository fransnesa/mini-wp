mn":16}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":91,"column":17}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":91,"column":29}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":91,"column":31}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":91,"column":36}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":84,"column":0},"generated":{"line":92,"column":0}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":49,"column":0},"generated":{"line":93,"column":0}},{"source":"src/components/listcontent.vue","name":null,"original":{"line":47,"column":0},"generated":{"line":94,"column":0}}],"sources":{"src/components/listcontent.vue":"<template>\n<div id=\"content-container-outside\">\n<div id=content-container>\n        <div class=\"content\">\n            <div class=\"center\">\n                <div class=\"published-drafts\">\n                    <div id=\"container-pod\">\n                            <div class=\"btn-group\">\n                                    <button class=\"button\">Published</button>\n                                    <button class=\"button\">Drafts</button>\n                            </div>\n                    </div>\n                    <div>\n                        <input type=\"text\" id=\"search-article\" placeholder=\"Search..\" title=\"Type in a category\">\n                    </div>\n                </div>\n                <div class=\"list-article\" v-for=\"(article,index) in articles\" :key=\"index\">\n                    <div id=\"name-article\">\n                        <div class=\"photo-container\">\n                            <img :src=\"article.featured_image\" alt=\"foto\" id=\"photo\"/>\n                            <h4>{{article.title}}</h4>\n                            <p v-html=\"String(article.content).substr(0,500)\"></p>\n                        </div>\n                    <h5>Created by: {{article.author}}</h5>\n                    <h6>{{String(new Date(article.createdAt)).substr(0,21)}}</h6>\n                    <div>\n                    <h6>tags:</h6>\n                        <a href=\"#\" v-for=\"(tag,index) in article.articletags\" :key=\"index\">\n                        <vs-chip>{{tag}}</vs-chip>\n                        </a>\n                    </div>\n                        <div class=\"btn-group\">\n                            <button class=\"button\" @click.prevent=\"clickEdit(article)\"><i class=\"fa fa-pencil\"></i> Edit</button>\n                            <button class=\"button\" @click.prevent=\"deleteData(article._id)\"><i class=\"fa fa-trash\"></i> Trash</button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n</div>\n</div>\n</template>\n\n<script>\n// let baseurl = \"http://localhost:3000\"\nlet baseurl = \"http://34.87.55.207\"\nexport default {\n    props : [\"articles\"],\n    methods:{\n            deleteData(id){\n                console.log(id)\n                Swal.fire({\n                    title: \"Are you sure?\",\n                    text: \"You won't be able to revert this!\",\n                    type: \"warning\",\n                    showCancelButton: true,\n                    confirmButtonColor: \"#3085d6\",\n                    cancelButtonColor: \"#d33\",\n                    confirmButtonText: \"Yes, delete it!\"\n                }).then(result=>{\n                    if(result.value){\n                        axios({\n                            method: 'DELETE',\n                            url:`${baseurl}/miniWp/`+id,\n                            headers:{\n                                token: localStorage.getItem('token')\n                            }\n                        }).then((response)=>{\n                    Swal.fire(\"Success Delete\");\n                    this.$emit(\"successDelete\")\n                }).catch(error => console.log(error))                       \n                    } else {\n                        Swal.close()\n                    }\n                }).catch(error=>{\n                    let message =\n            (error.response.data && error.response.data.message) ||\n            \"failed to Delete\";\n            Swal.fire(\"Error!\", message, \"error\");\n                })\n            },\n            clickEdit(input) {\n                this.$emit(\"updateFile\", input);\n            }\n    }\n}\n</script>\n\n<style scoped>\n#photo-container{\n    width: 100px;\n    height: 100px;\n    display: flex;\n    justify-content: flex-start;\n    flex-direction: row;\n    flex-wrap: wrap;\n}\n#photo {\n    width: 300px;\n    height: 200px;\n    margin-left: 20px;\n}\n#content-container{\n    overflow: scroll;\n}\n#content-container-outside{\n    overflow: scroll;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"2d50c5a0fd5003bd9ff53c69fb4fe30f","cacheData":{"env":{}}}